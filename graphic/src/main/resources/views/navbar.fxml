<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER_LEFT" prefHeight="60.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dictionary.graphic.controllers.NavbarController">
   <children>
      <Button mnemonicParsing="false" onAction="#switchToEnglishVietnamese" styleClass="menu-btn" text="%english_vietnamese" textOverrun="WORD_ELLIPSIS">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Button>
      <Button mnemonicParsing="false" onAction="#switchToVietnameseEnglish" text="%vietnamese_english" textOverrun="WORD_ELLIPSIS">
         <HBox.margin>
            <Insets />
         </HBox.margin>
         <styleClass>
            <String fx:value="menu-btn" />
            <String fx:value="btn-selected" />
         </styleClass>
      </Button>
      <Button mnemonicParsing="false" onAction="#switchToTranslate" text="%translate" textOverrun="WORD_ELLIPSIS">
         <styleClass>
            <String fx:value="menu-btn" />
            <String fx:value="btn-selected" />
         </styleClass>
      </Button>
      <Button mnemonicParsing="false" onAction="#switchToSettings" text="%settings" textOverrun="WORD_ELLIPSIS">
         <styleClass>
            <String fx:value="menu-btn" />
            <String fx:value="btn-selected" />
         </styleClass>
      </Button>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets left="20.0" />
   </padding>
   <styleClass>
      <String fx:value="menu-bar" />
      <String fx:value="dropshad" />
   </styleClass>
</HBox>
